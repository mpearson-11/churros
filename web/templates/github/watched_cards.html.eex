<%= for {card, index} <- @cards do %>
  <%= if Map.has_key?(card, "content") do %>
    <div class="card mt-2 mb-1">
      <div class="card-header" data-socket-card-index='<%= index %>' id='card-<%= card |> content |> number %>'>
        <div class="h4"><%= card |> content |> number %></div>
      </div>
      <div class="card-body">
        <h5><%= card |> content |> title %></h5>
      </div>
    </div>
  <%= end %>
<%= end %>
