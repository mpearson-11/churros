<%= for {card, index} <- @cards do %>
  <%= if Map.has_key?(card, "content") do %>
    <div class="card mt-2 mb-1">
      <div class="card-header text-white bg-primary" data-socket-card-index='<%= index %>' id='card-<%= card |> content |> number %>'>
        <strong><%= card |> content |> number %></strong>
      </div>
      <div class="card-body">
        <h3><%= card |> content |> title %></h3>
      </div>
    </div>
  <%= end %>
<%= end %>
