<div id="live-data" data-socket-name='<%= @id %>' data-socketwatching='<%= @watching || "false" %>'>
  <div class="container">
    <div class="row mt-5 pt-5">
      <div class="col-10" >
        <h2 class="mt-3">Watching repository issues for: <%= @repo %> on project: <%= @number %></h2>
      </div>
      <div class="col-2">
        <span><div class="loading-spinner"></div></span>
      </div>
    </div>
  </div>
</div>