
<div class="container-fluid">
  <div class="row">
    <div class="col-8" id="live-data" data-socket-name='<%= @id %>' data-socket-watching='<%= @watching || "false" %>'>
      <h1 class="mt-3">Watching Repo: <%= @repo %></h1>
    </div>
    <div class="col-4 mt-2">
      <h3 class="mt-3">Watched PR's</h3>
      <div id="live-watched-data" class="card-column">
      </div>
    </div>
  </div>
</div>